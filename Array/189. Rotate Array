class Solution {
    public static void swap(int[] arr,int i,int j){
        while(i<j){
            int t=arr[i];
            arr[i]=arr[j];
            arr[j]=t;
            i++;
            j--;
        }
    }
    public void rotate(int[] nums, int k) {
        int n=nums.length;
        k=k%n;
        swap(nums,0,n-1);// reverse entire array
        swap(nums,0,k-1);
        swap(nums,k,n-1);
    }
}
