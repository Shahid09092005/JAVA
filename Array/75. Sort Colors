class Solution {
    public static void swap(int[] arr , int i,int j){
        int t=arr[i];
        arr[i]=arr[j];
        arr[j]=t;
    }
    public void sortColors(int[] nums) {
    int l=0; // 0
    int m=0; // for 1
    int h=nums.length-1; //for 2
    while(m<=h){
        if(nums[m]==2){
            swap(nums,m,h);
            h--;
        }
        else if(nums[m]==0){
            swap(nums,m,l);
            l++;
            m++;
        }
        else if(nums[m]==1){
            m++;
        }
    }
    
        
    }
}
